{
  "name": "nextjs-starter",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "format": "prettier . --write",
    "lint": "next lint --max-warnings=0",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,json}": "prettier --write"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "clsx": "^2.1.1",
    "next": "^14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "only-allow": "^1.2.1",
    "postcss": "^8",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-eslint": "^7.8.0"
  }
}
